{
    "cmake.configureOnOpen": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "hide-files.files": [],
    "CodeGPT.apiKey": "CodeGPT Plus Beta",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "always"
    },
}